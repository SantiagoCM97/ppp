program matrixmultiplication;

main() {
    var int matrix1[3][4], matrix2[4][6], result[3][6], i, j;

    %% matrix1 assigning
    for j = 0 to j < 4 {
        for i = 0 to i < 3 {
            matrix1[i][j] = i + j;
        }
    }
    
    %% matrix2 assigning
    for j = 0 to j < 6 {
        for i = 0 to i < 4 {
            matrix2[i][j] = i + j;
        }
    }

    result = matrix1 * matrix2;

    %% printing result
    for j = 0 to j < 6 {
        for i = 0 to i < 3 {
            print(result[i][j]);
        }
    }
}